{
  "fluxo": "feriados-nacionais",
  "contrato": {
    "entrada": [
      {
        "id": "id-ano-referencia",
        "nomeCampo": "anoReferencia",
        "obrigatorio": true
      }
    ],
    "saida": [
      {
        "id": "lista-feriados",
        "nomeCampo": "feriados",
        "valor": "{{java(trasformadorListaDeObjetos(variaveis$.variavel-lista-feriados, estruturas$.estrutura-feriados-raw, estruturas$.estrutura-feriados, mapeamentos$.mapeamento-feriados))}}"
      }
    ]
  },
  "orquestrador": {
    "passos": [
      {
        "ordem": 1,
        "id": "passo-consulta-feriados",
        "tipo": "API",
        "integracao": {
          "requisicao": {
            "metodo": "GET",
            "url": "https://brasilapi.com.br/api/feriados/v1/{{variaveis$.variavel-ano-referencia}}",
            "queryParams": null,
            "headers": null,
            "body": null
          },
          "resposta": {
            "body": [
              {
                "date": "String",
                "name": "String",
                "type": "String"
              }
            ]
          }
        }
      }
    ]
  },
  "metadados": {
    "variaveis": [
      {
        "id": "variavel-ano-referencia",
        "origem": {
          "tipoReferencia": "contrato",
          "idReferencia": "{{entrada$.id-ano-referencia}}"
        }
      },
      {
        "id": "variavel-lista-feriados",
        "origem": {
          "tipoReferencia": "orquestrador",
          "idReferencia": "{{api$.passo-consulta-feriados$.resposta$.<List>}}"
        }
      }
    ],
    "estruturas": [
      {
        "id": "estrutura-feriados-raw",
        "valor": {
          "date": "String",
          "name": "String",
          "type": "String"
        }
      },
      {
        "id": "estrutura-feriados",
        "valor": {
          "dataFeriado": "String",
          "nameFeriado": "String"
        }
      }
    ],
    "mapeamentos": [
      {
        "id": "mapeamento-feriados",
        "valor": [
          { "origem": "date", "destino": "dataFeriado" },
          { "origem": "name", "destino": "nameFeriado" }
        ]
      }
    ]
  }
}